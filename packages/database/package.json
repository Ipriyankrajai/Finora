{
  "name": "@finora/database",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "with-env": "dotenv -e ../../.env --",
    "db:generate": "bun run with-env prisma generate",
    "db:push": "bun run with-env prisma db push",
    "db:migrate": "bun run with-env prisma migrate dev",
    "db:studio": "bun run with-env prisma studio",
    "db:seed": "bun run with-env tsx src/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "zod-prisma-types": "^3.3.5"
  },
  "devDependencies": {
    "@finora/typescript-config": "workspace:*",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3"
  }
}
